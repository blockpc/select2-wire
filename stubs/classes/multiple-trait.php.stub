<?php

namespace App\Http\Livewire\Select2\Traits;

trait MultipleTrait
{
    public function initializeMultipleTrait()
    {
        $this->listeners = array_merge($this->listeners, [
            'set-[MODEL]' => 'set_[MODEL]'
        ]);
    }

    public function set_[MODEL](int $id)
    {
        $this->[MODEL]->id = $id;
    }
}